    "# traditional binary search\n",
    "def binary_search_5(item_list, to_find):\n",
    "    left = 0\n",
    "    right = len(item_list) - 1\n",
    "    while left <= right:\n",
    "        mid = (left + right) // 2\n",
    "        if item_list[mid] == to_find:\n",
    "            return mid\n",
    "        elif item_list[mid] < to_find:\n",
    "            left = mid + 1\n",
    "        else:\n",
    "            right = mid - 1\n",
    "    return -1  # Element not found"
   ]
  },
